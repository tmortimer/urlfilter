{
	"filters": [
		"redismysqlbloom",
		"redis",
		"mysql"
	],
	"redis":{
		"host":"bloom-redis-mysql-redisdb"
	},
	"mysql":{
		"host":"bloom-redis-mysql-mysqldb",
